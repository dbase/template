#: src/main.c:69
#, c-format
msgid "Hello, world!\n"
msgstr "Bonjour Monde !\n"

#: src/main.c:77
#, c-format
msgid "\t    Usage: %s [OPTION]...\n"
msgstr "\t    Usage: %s [OPTION]...\n"

#: src/main.c:80
msgid "\t    Print a friendly, customizable greeting.\n"
msgstr "\t    Print a friendly, customizable greeting.\n"

#: src/main.c:84
msgid ""
"\t    -h, --help          display this help and exit\n"
"\t    -v, --version       display version information and exit\n"
msgstr ""
"\t    -h, --help          display this help and exit\n"
"\t    -v, --version       display version information and exit\n"

#: src/main.c:89
msgid ""
"\t    -t, --traditional       use traditional greeting format\n"
"\t    -n, --next-generation   use next-generation greeting format\n"
"\t    -g, --greeting=TEXT     use TEXT as the greeting message\n"
msgstr ""
"\t    -t, --traditional       use traditional greeting format\n"
"\t    -n, --next-generation   use next-generation greeting format\n"
"\t    -g, --greeting=TEXT     use TEXT as the greeting message\n"

